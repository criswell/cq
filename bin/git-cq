#!/usr/bin/env bash

. $CQLIB_PATH/import.sh

import "common.sh"
import "output.sh"
import "usage.sh"

for i in "$@"
do
  case $i in
    init)
      CMD_INIT=yes
      shift
      ;;
    help)
      CMD_HELP=yes
      shift
      ;;
    -v|--verbose)
      VERBOSE=yes
      shift
      ;;
    -*)
      OPTIONS+=("${i#*=}")
      shift
      ;;
    *)
      PARAMETERS+=("${i#*=}")
      shift
      ;;
  esac
done

if [ -n "$CMD_HELP" ]; then
  usage
  exit 0
elif [ -n "$CMD_INIT" ]; then
  init_cq
  exit 0
fi


